const SPEED := 70

var timer := 0.0


func _physics_process(delta):
	position += transform.x * SPEED
	
	timer += delta
	if timer > 0.5:
		queue_free()


func _on_Area2D_area_entered(area : Area2D):
	area.get_parent().life -= 1
	queue_free()
"

